// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Member {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  username    String?
  passname    String?
  firstname   String?
  lastname    String?
  bankAccount String?
  bank        String?
  phone       String?
  line        String?
  email       String?
  createdAt   DateTime? @default(now())
  updatedAt   DateTime? @updatedAt
  deleted_at  DateTime? @map("deleted_at")
}

model Partner {
  id                 String    @id @default(auto()) @map("_id") @db.ObjectId
  userAG             String?
  originAG           String?
  percent            String?
  commission         String?
  overdue            String?
  adjustPercentage   String?
  pay                String?
  customerCommission String?
  recommender        String?
  createdAt          DateTime? @default(now())
  updatedAt          DateTime? @updatedAt
  deleted_at         DateTime? @map("deleted_at")
}

model InvestmentUser {
  id         String    @id @default(auto()) @map("_id") @db.ObjectId
  username   String?
  password   String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime? @updatedAt
  deleted_at DateTime? @map("deleted_at")
}

model MinusIncome {
  id              String    @id @default(auto()) @map("_id") @db.ObjectId
  negativeBalance String?
  transferOverdue String?
  pomotion        String?
  investment      String?
  createdAt       DateTime? @default(now())
  updatedAt       DateTime? @updatedAt
  deleted_at      DateTime? @map("deleted_at")
}

model PastAG {
  id         String    @id @default(auto()) @map("_id") @db.ObjectId
  online     String?
  comC       String?
  loseC      String?
  comM       String?
  loseM      String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime? @updatedAt
  deleted_at DateTime? @map("deleted_at")
}

model PastBETFLIX {
  id         String    @id @default(auto()) @map("_id") @db.ObjectId
  online     String?
  comC       String?
  loseC      String?
  loseM      String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime? @updatedAt
  deleted_at DateTime? @map("deleted_at")
}

model AgentIncomeSystem {
  id              String    @id @default(auto()) @map("_id") @db.ObjectId
  comSum          String?
  interestUFA     String?
  interestBETFLIX String?
  interesSum      String?
  percentage      String?
  totalLoss       String?
  income          String?
  createdAt       DateTime? @default(now())
  updatedAt       DateTime? @updatedAt
  deleted_at      DateTime? @map("deleted_at")
}

model MastertIncomeSystem {
  id                      String    @id @default(auto()) @map("_id") @db.ObjectId
  comSum                  String?
  interestUFA             String?
  interestBETFLIX         String?
  promotion               String?
  negativeBalance         String?
  comA                    String?
  fullPayAgent            String?
  actualPayAgent          String?
  totalLoss               String?
  percentage              String?
  income                  String?
  recommendedValueUFRUU   String?
  recommendedValueUFRUV   String?
  recommendedValueUFRUV02 String?
  createdAt               DateTime? @default(now())
  updatedAt               DateTime? @updatedAt
  deleted_at              DateTime? @map("deleted_at")
}

model TransferSummary {
  id               String    @id @default(auto()) @map("_id") @db.ObjectId
  recommendedValue String?
  agentTotal       String?
  masterTotal      String?
  investment       String?
  createdAt        DateTime? @default(now())
  updatedAt        DateTime? @updatedAt
  deleted_at       DateTime? @map("deleted_at")
}

model Admin {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  username    String?
  passname    String?
  firstname   String?
  lastname    String?
  bankAccount String?
  bank        String?
  phone       String?
  line        String?
  nickName    String?
  createdAt   DateTime? @default(now())
  updatedAt   DateTime? @updatedAt
  deleted_at  DateTime? @map("deleted_at")
}

model RightsManagement {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  page        String?
  rightToWork String?
  createdAt   DateTime? @default(now())
  updatedAt   DateTime? @updatedAt
  deleted_at  DateTime? @map("deleted_at")
}
